.search-results
	.search-container
		height 210px

	.right-col__search-btn
		color $white
		border-color $white

	.results
		@extend $mainContainer
		padding-top 33px

	.link
		display block
		width 74.3%
		margin-right 0
		margin-bottom 30px

	.results__rul
		@extend $smallerFontSize
		margin-bottom 40px
		span, a
			@extend $transitionDefault
			color $blueJeans
		a:hover
			color $blue
